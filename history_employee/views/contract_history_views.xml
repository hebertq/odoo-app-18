<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!-- List view of 'contract.history' model. -->
    <record id="contract_history_view_tree" model="ir.ui.view">
        <field name="name">contract.history.view.list</field>
        <field name="model">contract.history</field>
        <field name="arch" type="xml">
            <list string="contract History">
                <field name="employee" column_invisible="True"/>
                <field name="employee_name" column_invisible="True"/>
                <field name="updated_date"/>
                <field name="changed_field"/>
                <field name="current_value"/>
            </list>
        </field>
    </record>
</odoo>
